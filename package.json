{
  "name": "@mixspa/vue",
  "version": "0.6.0",
  "description": "This is a tools for mixspa with vue",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "babel": "babel src --out-dir lib --copy-files --no-copy-ignored --ignore 'src/**/*.spec.js'",
    "babel:watch": "watch 'yarn babel' ./src",
    "build": "yarn clean && yarn babel",
    "lint": "eslint src",
    "test": "jest --coverage --config config/jest.config.js",
    "test:watch": "watch 'yarn test' ./src",
    "start": "webpack-dev-server --config config/webpack.config.js --open"
  },
  "dependencies": {},
  "peerDependencies": {
    "@mixspa/core": "^0.6.3",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@mixspa/core": "^0.6.3",
    "@testing-library/dom": "^7.21.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/vue": "^5.0.3",
    "@vue/cli-plugin-babel": "^4.4.1",
    "@vue/cli-plugin-eslint": "^4.4.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "eslint": "^7.4.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-testing-library": "^3.2.0",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.6",
    "vue-loader": "^15.9.2",
    "vue-router": "^3.3.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "watch": "^1.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mixspa/mixspa-vue.git"
  },
  "keywords": [
    "vue",
    "mixspa"
  ],
  "author": "",
  "contributors": [
    "Xiaoqi Cao"
  ],
